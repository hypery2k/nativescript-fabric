{
  "scripts": {
    "ns-bundle": "ns-bundle",
    "build.plugin": "cd ../src && npm run build",
    "ci.tslint": "npm i && tslint --config '../tslint.json' 'app/**/*.ts' --exclude '**/node_modules/**'",
    "ci.android.build": "npm run build.plugin && tns prepare android && tns build android",
    "ci.ios.build": "npm run build.plugin && tns prepare ios && tns build ios",
    "publish-ios-bundle": "tns build ios --bundle --publish-app",
    "generate-android-snapshot": "generate-android-snapshot --targetArchs arm,arm64,ia32 --install",
    "start-android-bundle": "tns build android --bundle --run-app",
    "start-ios-bundle": "tns build ios --bundle --run-app",
    "build-android-bundle": "tns build android  --env.snapshot --env.uglify --env.aot",
    "build-ios-bundle": "tns build ios --env.uglify --env.aot"
  },
  "nativescript": {
    "id": "org.nativescript.demoangular",
    "tns-ios": {
      "version": "3.4.1"
    },
    "tns-android": {
      "version": "3.4.1"
    }
  },
  "dependencies": {
    "@angular/common": "5.1.3",
    "@angular/compiler": "5.1.3",
    "@angular/core": "5.1.3",
    "@angular/forms": "5.1.3",
    "@angular/http": "5.1.3",
    "@angular/platform-browser": "5.1.3",
    "@angular/platform-browser-dynamic": "5.1.3",
    "@angular/router": "5.1.3",
    "nativescript-angular": "5.1.0",
    "nativescript-fabric": "file:../src",
    "nativescript-theme-core": "1.0.4",
    "nativescript-unit-test-runner": "0.3.4",
    "rxjs": "5.5.6",
    "tns-core-modules": "3.4.0",
    "zone.js": "0.8.19"
  },
  "devDependencies": {
    "@angular/compiler-cli": "5.1.3",
    "@ngtools/webpack": "~1.8.2",
    "babel-traverse": "6.12.0",
    "babel-types": "6.11.1",
    "babylon": "6.8.4",
    "copy-webpack-plugin": "~4.0.1",
    "css-loader": "~0.28.7",
    "extract-text-webpack-plugin": "~3.0.0",
    "filewalker": "0.1.2",
    "jasmine-core": "^2.8.0",
    "karma": "^2.0.0",
    "karma-jasmine": "^1.1.1",
    "karma-nativescript-launcher": "^0.4.0",
    "lazy": "1.0.11",
    "nativescript": "3.4.1",
    "nativescript-css-loader": "~0.26.0",
    "nativescript-dev-typescript": "^0.6.0",
    "nativescript-dev-webpack": "^0.9.0",
    "nativescript-worker-loader": "~0.8.1",
    "raw-loader": "~0.5.1",
    "resolve-url-loader": "~2.1.0",
    "tslint": "~5.4.3",
    "typescript": "~2.4.2",
    "uglifyjs-webpack-plugin": "~1.1.6",
    "webpack": "~3.8.1",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-sources": "~1.0.1"
  }
}
